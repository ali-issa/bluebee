    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/p5": "^1.7.6",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@types/three": "^0.176.0",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "pg-cloudflare": "1.1.1"
  },
  "packageManager": "pnpm@10.3.0",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "packageManger": "pnpm@9.7.1"
}
